      <errorMessage>Request completed successfully</errorMessage>
      <subscriber>
      ...
      </subscriber>
      <subscriber>
      ...
      </subscriber>
      ...
    </SearchSubscribersResponse>
  </se:Body>
</se:Envelope>{EXAMPLE_END}
      </xsd:documentation>
